{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [

    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "axios@latest",
      "destination": "wwwroot/lib/axios/",
      "files": [
        "dist/axios.min.js"
      ]
    }
  ]
}